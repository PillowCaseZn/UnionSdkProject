<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pillowcase.demo"
    android:installLocation="auto">

    <application
        android:name="com.pillowcase.demo.GameApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.pillowcase.demo.GameActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- 指定渠道Application-->
        <!-- 如果是直接继承使用com.pillowcase.union.UnionApplication，则可不需要配置该meta-data属性-->
        <meta-data
            android:name="APPLICATION_PROXY_NAME"
            android:value="com.pillowcase.union.channels.DefaultSdkApplication" />
    </application>

</manifest>